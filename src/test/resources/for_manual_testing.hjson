{
  "includeOnlyFromDirectories": false,
  "directories": [
    "./src/main/java"
  ],
  "nodeStyles": {
    "default": {
      "fontsize": 20,
      "shape": "box"
    },
    "java-parser-lib": {
      "color": "orange",
      "inheritDefault": false
    },
    "graphviz-lib": {
      "style": "filled",
      "fillcolor": "green"
    },
    "gson-lib": {
      "style": "filled",
      "fillcolor": "orange",
      "shape": "null"
    }
  },
  "edgeStyles": {
    "default": {
      "color": "black",
      "weight": 1
    },
    "solid-green": {
      "style": "solid",
      "color": "green"
    }
  },
  "definitions": [
    {
      "packages": "com.github.gzougianos\\.(.*)",
      "as": "$1"
    },
    {
      "packages": "java.*,lombok.*",
      "as": ""
    },
    {
      "packages": "com.google.gson.*",
      "as": "Gson",
      "nodeStyle": "gson-lib"
    },
    {
      "packages": "com.github.javaparser.*",
      "as": "JavaParser Lib",
      "nodeStyle": "java-parser-lib"
    },
    {
      "packages": "guru.nidi.*",
      "as": "Graphviz",
      "nodeStyle": "graphviz-lib",
      "edgeInStyle": "solid-green"
    }
  ],
  "clusters": [
    {
      "packages": "${PACKAGRAPH_PACKAGE_PATTERN}",
      "name": "Packagraph",
      "style": {
        "color": "gray",
        "label": "packgraph internals",
        "fontsize": 18,
        "fontcolor": "gray"
      }
    },
    {
      "packages": ".*",
      "name": "Externals",
      "style": {
        "color": "gray",
        "label": "External Dependencies",
        "fontsize": 18,
        "fontcolor": "gray"
      }
    }
  ],
  "constants": [
    {
      "name": "BLUE",
      "value": "#0000FF"
    },
    {
      "name": "PACKAGRAPH_PACKAGE_PATTERN",
      "value": "packagraph.*"
    }
  ],
  "output": {
    "path": "./packagraph.png",
    "overwrite": true,
    "style": {
      "label": "ManualTestingSample",
      "fontsize": 18,
      "fontname": "Tahoma",
      "fontcolor": "purple",
      "labeljust": "r",
      "dpi": 75
    }
  }
}