//We are at ./target/test-classes
include source directory '../../src/main/java';

show maingraph with style 'maingraph';
define style 'maingraph' as 'dpi=100;label=PackagraphInternals;fontsize=18;fontname=Tahoma';

define constant 'ANTLR_NAMESPACE' as 'org.antlr.*';

show nodes 'java.*' as '';
show nodes 'lombok.*' as '';
show nodes '.*' with style 'shape=box';
show nodes 'com.github.gzougianos\.(.*)' as '$1';
show nodes '${ANTLR_NAMESPACE}' as 'ANTLR' with style 'infrastructure';
show nodes 'guru.nidi.*' as 'Graphviz' with style 'infrastructure';
show nodes 'com.github.javaparser.*' as 'JavaParser' with style 'infrastructure';

show edges to '${ANTLR_NAMESPACE}' with style 'green_edge';

define style 'infrastructure' as 'style=filled;fillcolor=lightblue';
define style 'green_edge' as 'color=green;style=solid';


export as '.svg' into '../../target/manual_testing.svg' by overwriting;