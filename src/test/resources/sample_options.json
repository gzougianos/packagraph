{
  "includeOnlyFromDirectories": false,
  "directories": [
    "src/main/java",
    "src/test/java"
  ],
  "definitions": [
    {
      "packages": "java.util.*",
      "as": "java"
    },
    {
      "packages": "com.github.com",
      "as": ""
    },
    {
      "packages": "com.something\\.(.*)",
      "as": "$1"
    }
  ],
  "clusters": [
    {
      "packages": "java.util.*",
      "name": "something",
      "style": {
        "color": "red",
        "label": "Something",
        "fontsize": 42
      }
    }
  ],
  "constants": [
    {
      "name": "MAIN_GRAPH_FONT_COLOR ",
      "value": "pink "
    }
  ],
  "output": {
    "path": "target/packagraph.png",
    "overwrite": true,
    "style": {
      "label": "a_label",
      "fontsize": 24,
      "fontname": "Tahoma",
      "fontcolor": "${MAIN_GRAPH_FONT_COLOR}"
    }
  },
  "globalStyle": {
    "shape": "rectangle",
    "style": "rounded",
    "fillcolor": "lightblue"
  },
  "globalEdgeStyle": {
    "label": "Sample Edge",
    "color": "blue",
    "style": "dashed"
  }
}