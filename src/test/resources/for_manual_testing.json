{
  "includeOnlyFromDirectories": false,
  "directories": [
    "./src/main/java"
  ],
  "definitions": [
    {
      "packages": "java.*,lombok.*",
      "as": ""
    },
    {
      "packages": "com.github.javaparser.*",
      "as": "JavaParser",
      "style": {
        "color": "blue",
        "inheritGlobal": false
      }
    },
    {
      "packages": "guru.nidi.*",
      "as": "Graphviz",
      "style": {
        "style": "filled",
        "fillcolor": "green"
      }
    }
  ],
  "globalStyle": {
    "fontsize": 20,
    "shape": "box"
  },
  "outputImage": {
    "path": "./packagraph.png",
    "overwrite": true
  }
}